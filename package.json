{
  "name": "surveyapp",
  "version": "1.0.0",
  "main": "index.js",
  "author": {
    "name": "Daniel <daniel.dungngo@gmail.com>"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "find app/* ! \\( -name assets -o -name img -o -name \"*.png\" \\) -exec rm -rf {} +",
    "build": "SET NODE_ENV=production&&webpack -p --env.production --progress --colors",
    "lint": "tslint -p . --type-check -t stylish",
    "g:log": "git log --pretty=format:\"%h - %an, %ar : %s\"",
    "g:commit": "git add . & git commit -m ",
    "g:push": "git pull & git push origin master",
    "g:pull": "git pull origin son-dev",
    "dev": "SET NODE_ENV=development&&webpack-dev-server --env.development",
    "dev:d": "webpack-dashboard -- webpack-dev-server",
    "refresh": "rm -rf node_modules && rm yarn.lock && rm yarn-error.log && yarn",
    "predeploy": "yarn build",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "moment": "^2.22.2",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "react": "^16.4.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-swipeable-views": "^0.12.13",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^3.3.36",
    "@types/classnames": "^2.2.4",
    "@types/node": "^10.3.1",
    "@types/react": "^16.3.16",
    "@types/react-custom-scrollbars": "^4.0.3",
    "@types/react-dom": "^16.0.6",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-thunk": "^2.1.0",
    "@types/webpack": "^4.4.0",
    "@types/webpack-dev-server": "^2.9.4",
    "@types/webpack-env": "^1.13.6",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "react-hot-loader": "^4.3.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.1",
    "tslib": "^1.9.2",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-misc-rules": "^3.4.0",
    "typescript": "^3.0.0-dev.20180606",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  }
}
